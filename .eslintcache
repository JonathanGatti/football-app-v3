.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/pages/CreateTeam.tsx",["118"],"/Users/jonathan/Desktop/football app v3/football-app/src/actions/types.ts",[],["119","120"],"/Users/jonathan/Desktop/football app v3/football-app/src/actions/index.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/utils/getPlayersAndFilterByName.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/components/SearchResultList.tsx",[],"/Users/jonathan/Desktop/football app v3/football-app/src/components/SearchPlayerForm.tsx",["121"],"import React, { useState } from 'react';\nimport { postPlayer } from '../api/localApi';\nimport { Team, Player } from '../interfaces';\nimport SearchResultList from './SearchResultList';\nimport { getPlayersAndFilterByName } from '../utils/getPlayersAndFilterByName';\nimport { Header, Modal, List, Icon, Input, Divider } from 'semantic-ui-react';\nimport { SearchPlayerFormProps } from '../interfaces';\n\nfunction SearchPlayerForm({\n  team,\n  setTeam,\n  setRating,\n  index,\n}: SearchPlayerFormProps) {\n  const [name, setName] = useState('');\n  const [searchResult, setSearchResult] = useState([]);\n  const [open, setOpen] = useState(false);\n\n  const handleSubmit = async () => {\n    setOpen(true);\n    try {\n      let res = await getPlayersAndFilterByName(name);\n      setSearchResult(res);\n    } catch (e) {\n      console.log(e);\n    }\n  };\n\n  const handleChange = (e: any) => setName(e.target.value);\n\n  const reset = () => setName('');\n\n  const handleClick = async (player: Player, i: number) => {\n    let newPlayer = await postPlayer(player);\n    team.teamPlayers[i] = newPlayer;\n    if (newPlayer.rating !== null) {\n      setRating(Math.floor(team.rating + parseInt(newPlayer.rating)));\n    }\n    setTeam({ ...team });\n  };\n  return (\n    <>\n      <form\n        onSubmit={(e) => {\n          e.preventDefault();\n          handleSubmit();\n          reset();\n        }}\n      >\n        <Input\n          type=\"text\"\n          value={name}\n          placeholder=\"Search...\"\n          onChange={handleChange}\n        />\n      </form>\n      <Modal\n        basic\n        onClose={() => setOpen(false)}\n        onOpen={() => setOpen(true)}\n        open={open}\n        size=\"small\"\n      >\n        <Header icon>List of Players</Header>\n        <Modal.Content>\n          {searchResult === undefined ? (\n            <div>player not found</div>\n          ) : (\n            <List>\n              {searchResult.length === 0 ? (\n                <Icon loading name=\"spinner\" />\n              ) : (\n                searchResult.map((player: Player) => (\n                  <>\n                    <SearchResultList\n                      index={index}\n                      onClick={handleClick}\n                      onOpen={setOpen}\n                      player={player}\n                    />\n                    <Divider />\n                  </>\n                ))\n              )}\n            </List>\n          )}\n        </Modal.Content>\n      </Modal>\n    </>\n  );\n}\n\nexport default SearchPlayerForm;\n",["122","123"],"/Users/jonathan/Desktop/football app v3/football-app/src/utils/defaultTeam.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/reportWebVitals.ts",[],["124","125"],"/Users/jonathan/Desktop/football app v3/football-app/src/reducers/teamReducer.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/pages/Home.tsx",["126"],"/Users/jonathan/Desktop/football app v3/football-app/src/utils/teamModules.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/config.ts",[],["127","128"],"/Users/jonathan/Desktop/football app v3/football-app/src/components/TeamForm.tsx",["129","130"],"/Users/jonathan/Desktop/football app v3/football-app/src/api/localApi.ts",["131"],"import axios from 'axios';\nimport {getPlayerRatings} from './externalApi';\nimport {Player} from '../interfaces';\n\ninterface Data {\n  teamName: string;\n  teamPlayers: Player[];\n  teamModule: String[];\n  userId: string;\n  logo: string;\n}\n\nexport const getData = async () => {\n  try {\n    const res = await axios.get('http://127.0.0.1:8080/api/teams');\n    return res.data;\n  }\n  catch(e) {\n    console.log(e)\n  }\n}\nexport const postData = (data: Data) => {\n  axios.post('http://127.0.0.1:8080/api/teams',\n    data,\n     {\n      headers: {\n        'Content-Type': 'application/json',\n        }\n    })\n    .catch(e => {\n      console.log(e)\n    })\n}\n\nexport const postPlayer = async (data: Player) => {\n  let newPlayer;\n  const res = await getPlayers()\n  const player = res.filter((player: Player) => (\n    player.player_id === data.player_id\n  ));\n  newPlayer = player[0];\n  if(newPlayer === undefined){\n    const playerRating = await getPlayerRatings(data.player_id)\n    newPlayer = {...data, rating: playerRating};\n      axios.post('http://127.0.0.1:8080/api/players',\n      newPlayer,\n       {\n        headers: {\n          'Content-Type': 'application/json',\n          }\n      })\n      .catch(e => {\n        console.log(e)\n      })\n  }\n  return newPlayer;\n}\n\nexport const getPlayers = async () => {\n  try {\n    const res = await axios.get('http://127.0.0.1:8080/api/players');\n    return res.data;\n  }\n  catch(e) {\n    console.log(e);\n  }\n}\n\nexport const getPlayerById = async (id: number) => {\n  const res = await axios.get(`http://127.0.0.1:8080/api/players/${id}`)\n  return res;\n}\nexport const deleteTeam = (id: string) => {\n  axios.delete(`http://127.0.0.1:8080/api/teams/${id}`)\n}\n\nconst deletePlayers = async () => {\n  const res = await getPlayers();\n  res.map((player: Player) => (\n    axios.delete(`http://127.0.0.1:8080/api/players/${player._id}`)\n  ))\n}","/Users/jonathan/Desktop/football app v3/football-app/src/pages/TeamsList.tsx",["132"],"/Users/jonathan/Desktop/football app v3/football-app/src/pages/ShowTeam.tsx",["133","134","135"],"/Users/jonathan/Desktop/football app v3/football-app/src/App.tsx",["136"],"/Users/jonathan/Desktop/football app v3/football-app/src/reducers/authReducer.ts",["137"],"import {SIGN_IN, SIGN_OUT} from '../actions/types';\n\nconst INITIAL_STATE = {\n  isSignedIn: null,\n  userId: null\n}\n\ninterface ActionTypes {\n  type: string;\n  payload: number | string\n}\n\nexport default (state = INITIAL_STATE, action: ActionTypes) => {\n  switch(action.type){\n    case SIGN_IN:\n      return { ...state, isSignedIn: true, userId: action.payload}\n    case SIGN_OUT:\n      return { ...state, isSignedIn: false, userId: null }\n    default:\n      return state;\n  }\n}","/Users/jonathan/Desktop/football app v3/football-app/src/api/externalApi.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/components/Header.tsx",[],"/Users/jonathan/Desktop/football app v3/football-app/src/history.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/components/GoogleAuth.tsx",["138"],"import React, { useEffect } from 'react';\nimport { clientId } from '../config';\nimport { connect } from 'react-redux';\nimport { signIn, signOut } from '../actions';\nimport { GoogleAuthProps } from '../interfaces';\n\nfunction GoogleAuth({ isSignedIn, signIn, signOut }: GoogleAuthProps) {\n  useEffect(() => {\n    window.gapi.load('client:auth2', () => {\n      window.gapi.client\n        .init({\n          clientId: clientId,\n          scope: 'email',\n        })\n        .then(() => {\n          let auth = window.gapi.auth2.getAuthInstance();\n          onAuthChange(auth.isSignedIn.get());\n          auth.isSignedIn.listen(onAuthChange);\n        });\n    });\n  }, []);\n\n  const onAuthChange = (isSignedIn: boolean) => {\n    let auth = window.gapi.auth2.getAuthInstance();\n    if (isSignedIn) {\n      signIn(auth.currentUser.get().getId());\n    } else {\n      signOut();\n    }\n  };\n\n  const handleSignInButton = () => {\n    window.gapi.auth2.getAuthInstance().signIn();\n  };\n\n  const handleSignOutButton = () => {\n    window.gapi.auth2.getAuthInstance().signOut();\n  };\n\n  const renderAuthButton = () => {\n    if (isSignedIn === null) {\n      return null;\n    }\n    if (isSignedIn) {\n      return (\n        <button onClick={handleSignOutButton} className=\"ui red button\">\n          <i className=\"google icon\" />\n          Sign Out\n        </button>\n      );\n    } else if (!isSignedIn) {\n      return (\n        <button onClick={handleSignInButton} className=\"ui red button\">\n          <i className=\"google icon\" />\n          Sign In with Google\n        </button>\n      );\n    }\n  };\n\n  return <div>{renderAuthButton()}</div>;\n}\n\ninterface stateProps {\n  auth: {\n    isSignedIn: boolean | null;\n    userId: string | number | null;\n  };\n}\nconst mapStateToProps = (state: stateProps) => {\n  return { isSignedIn: state.auth.isSignedIn, userId: state.auth.userId };\n};\nexport default connect(mapStateToProps, { signIn, signOut })(GoogleAuth);\n","/Users/jonathan/Desktop/football app v3/football-app/src/pages/TeamFight.tsx",["139"],{"ruleId":"140","severity":1,"message":"141","line":1,"column":17,"nodeType":"142","messageId":"143","endLine":1,"endColumn":26},{"ruleId":"144","replacedBy":"145"},{"ruleId":"146","replacedBy":"147"},{"ruleId":"140","severity":1,"message":"148","line":3,"column":10,"nodeType":"142","messageId":"143","endLine":3,"endColumn":14},{"ruleId":"144","replacedBy":"149"},{"ruleId":"146","replacedBy":"150"},{"ruleId":"144","replacedBy":"151"},{"ruleId":"146","replacedBy":"152"},{"ruleId":"153","severity":1,"message":"154","line":55,"column":9,"nodeType":"155","endLine":55,"endColumn":29},{"ruleId":"144","replacedBy":"156"},{"ruleId":"146","replacedBy":"157"},{"ruleId":"140","severity":1,"message":"141","line":1,"column":17,"nodeType":"142","messageId":"143","endLine":1,"endColumn":26},{"ruleId":"153","severity":1,"message":"154","line":89,"column":7,"nodeType":"155","endLine":89,"endColumn":30},{"ruleId":"140