rs/jonathan/Desktop/football app v3/football-app/src/components/SearchPlayerForm.tsx",["137"],"import React, { useState } from 'react';\nimport { postPlayer } from '../api/localApi';\nimport { Team, Player } from '../interfaces';\nimport SearchResultList from './SearchResultList';\nimport { getPlayersAndFilterByName } from '../utils/getPlayersAndFilterByName';\nimport { Header, Modal, List, Icon, Input, Divider } from 'semantic-ui-react';\nimport { SearchPlayerFormProps } from '../interfaces';\n\nfunction SearchPlayerForm({\n  team,\n  setTeam,\n  setRating,\n  index,\n}: SearchPlayerFormProps) {\n  const [name, setName] = useState('');\n  const [searchResult, setSearchResult] = useState([]);\n  const [open, setOpen] = useState(false);\n\n  const handleSubmit = async () => {\n    setOpen(true);\n    try {\n      let res = await getPlayersAndFilterByName(name);\n      setSearchResult(res);\n    } catch (e) {\n      console.log(e);\n    }\n  };\n\n  const handleChange = (e: any) => setName(e.target.value);\n\n  const reset = () => setName('');\n\n  const handleClick = async (player: Player, i: number) => {\n    let newPlayer = await postPlayer(player);\n    team.teamPlayers[i] = newPlayer;\n    if (newPlayer.rating !== null) {\n      setRating(Math.floor(team.rating + parseInt(newPlayer.rating)));\n    }\n    setTeam({ ...team });\n  };\n  return (\n    <>\n      <form\n        onSubmit={(e) => {\n          e.preventDefault();\n          handleSubmit();\n          reset();\n        }}\n      >\n        <Input\n          type=\"text\"\n          value={name}\n          placeholder=\"Search...\"\n          onChange={handleChange}\n        />\n      </form>\n      <Modal\n        basic\n        onClose={() => setOpen(false)}\n        onOpen={() => setOpen(true)}\n        open={open}\n        size=\"small\"\n      >\n        <Header icon>List of Players</Header>\n        <Modal.Content>\n          {searchResult === undefined ? (\n            <div>player not found</div>\n          ) : (\n            <List>\n              {searchResult.length === 0 ? (\n                <Icon loading name=\"spinner\" />\n              ) : (\n                searchResult.map((player: Player) => (\n                  <>\n                    <SearchResultList\n                      key={player.player_id}\n                      index={index}\n                      onClick={handleClick}\n                      onOpen={setOpen}\n                      player={player}\n                    />\n                    <Divider />\n                  </>\n                ))\n              )}\n            </List>\n          )}\n        </Modal.Content>\n      </Modal>\n    </>\n  );\n}\n\nexport default SearchPlayerForm;\n","/Users/jonathan/Desktop/football app v3/football-app/src/utils/defaultTeam.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/reportWebVitals.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/reducers/teamReducer.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/pages/Home.tsx",["138"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { Button } from 'semantic-ui-react';\nimport image from '../assets/imgs/home_page_img.png';\nimport { HomePage } from '../styles/styledComponents';\n\nfunction Home() {\n  return (\n    <HomePage>\n      <div className=\"header\">\n        <div className=\"text\">\n          <h1>The Football App</h1>\n          <h3>\n            Lorem ipsum dolor sit amet consectetur adipisicing elit. Nostrum\n            nihil odit molestias! Laborum, voluptatibus quis dolorum tenetur\n            asperiores dignissimos error nihil ut in fugit ad quibusdam id\n            perspiciatis nesciunt delectus.\n          </h3>\n          <div className=\"buttons\">\n            <Button\n              as={Link}\n              to=\"team/create\"\n              attached=\"bottom\"\n              content=\"Create Your Team\"\n            />\n            <Button\n             