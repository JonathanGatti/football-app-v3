{\n    let auth = window.gapi.auth2.getAuthInstance();\n    if (isSignedIn) {\n      signIn(auth.currentUser.get().getId());\n    } else {\n      signOut();\n    }\n  };\n\n  const handleSignInButton = () => {\n    window.gapi.auth2.getAuthInstance().signIn();\n  };\n\n  const handleSignOutButton = () => {\n    window.gapi.auth2.getAuthInstance().signOut();\n  };\n\n  const renderAuthButton = () => {\n    if (isSignedIn === null) {\n      return null;\n    }\n    if (isSignedIn) {\n      return (\n        <button onClick={handleSignOutButton} className=\"ui red button\">\n          <i className=\"google icon\" />\n          Sign Out\n        </button>\n      );\n    } else if (!isSignedIn) {\n      return (\n        <button onClick={handleSignInButton} className=\"ui red button\">\n          <i className=\"google icon\" />\n          Sign In with Google\n        </button>\n      );\n    }\n  };\n\n  return <div>{renderAuthButton()}</div>;\n}\n\ninterface stateProps {\n  auth: {\n    isSignedIn: boolean | null;\n    userId: string | number | null;\n  };\n}\nconst mapStateToProps = (state: stateProps) => {\n  return { isSignedIn: state.auth.isSignedIn, userId: state.auth.userId };\n};\nexport default connect(mapStateToProps, { signIn, signOut })(GoogleAuth);\n","/Users/jonathan/Desktop/football app v3/football-app/src/pages/TeamFight.tsx",["142"],"/Users/jonathan/Desktop/football app v3/football-app/src/pages/TeamLayout.tsx",["143"],{"ruleId":"144","severity":1,"message":"145","line":1,"column":17,"nodeType":"146","messageId":"147","endLine":1,"endColumn":26},{"ruleId":"148","replacedBy":"149"},{"ruleId":"150","replacedBy":"151"},{"ruleId":"144","severity":1,"message":"152","line":3,"column":10,"nodeType":"146","messageId":"147","endLine":3,"endColumn":14},{"ruleId":"148","replacedBy":"153"},{"ruleId":"150","replacedBy":"154"},{"ruleId":"148","replacedBy":"155"},{"ruleId":"150","replacedBy":"156"},{"ruleId":"157","severity":1,"message":"158","line":55,"column":9,"node