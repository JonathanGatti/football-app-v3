tball-app/src/reducers/index.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/pages/CreateTeam.tsx",["121"],"/Users/jonathan/Desktop/football app v3/football-app/src/actions/types.ts",[],["122","123"],"/Users/jonathan/Desktop/football app v3/football-app/src/actions/index.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/utils/getPlayersAndFilterByName.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/components/SearchResultList.tsx",[],"/Users/jonathan/Desktop/football app v3/football-app/src/components/SearchPlayerForm.tsx",["124"],"import React, { useState } from 'react';\nimport { postPlayer } from '../api/localApi';\nimport { Team, Player } from '../interfaces';\nimport SearchResultList from './SearchResultList';\nimport { getPlayersAndFilterByName } from '../utils/getPlayersAndFilterByName';\nimport { Header, Modal, List, Icon, Input, Divider } from 'semantic-ui-react';\nimport { SearchPlayerFormProps } from '../interfaces';\n\nfunction SearchPlayerForm({\n  team,\n  setTeam,\n  setRating,\n  index,\n}: SearchPlayerFormProps) {\n  const [name, setName] = useState('');\n  const [searchResult, setSearchResult] = useState([]);\n  const [open, setOpen] = useState(false);\n\n  const handleSubmit = async () => {\n    setOpen(true);\n    try {\n      let res = await getPlayersAndFilterByName(name);\n      setSearchResult(res);\n    } catch (e) {\n      console.log(e);\n    }\n  };\n\n  const handleChange = (e: any) => setName(e.target.value);\n\n  const reset = () => setName('');\n\n  const handleClick = async (player: Player, i: number) => {\n    let newPlayer = await postPlayer(player);\n    team.teamPlayers[i] = newPlayer;\n    if (newPlayer.rating !== null) {\n      setRating(Math.floor(team.rating + parseInt(newPlayer.rating)));\n    }\n    setTeam({ ...team });\n  };\n  return (\n    <>\n      <form\n        onSubmit={(e) => {\n          e.preventDefault();\n          handleSubmit();\n          reset();\n        }}\n      >\n        <Input\n          type=\"text\"\n          value={name}\n          placeholder=\"Search...\"\n          onChange={handleChange}\n        />\n      </form>\n      <Modal\n        basic\n        onClose={() => setOpen(false)}\n        onOpen={() => setOpen(true)}\n        open={open}\n        size=\"small\"\n      >\n        <Header icon>List of Players</Header>\n        <Modal.Content>\n          {searchResult === undefined ? (\n            <div>player not found</div>\n          ) : (\n            <List>\n              {searchResult.length === 0 ? (\n                <Icon loading name=\"spinner\" />\n              ) : (\n                searchResult.map((player: Player) => (\n                  <>\n                    <SearchResultList\n                      index={index}\n                      onClick={handleClick}\n                      onOpen={setOpen}\n                      player={player}\n                    />\n                    <Divider />\n                  </>\n                ))\n              )}\n            </List>\n          )}\n        </Modal.Content>\n      </Modal>\n    </>\n  );\n}\n\nexport default SearchPlayerForm;\n","/Users/jonathan/Desktop/football app v3/football-app/src/utils/defaultTeam.ts",[],["125","126"],"/Users/jonathan/Desktop/football app v3/football-app/src/reportWebVitals.ts",[],["127","128"],"/Users/jonathan/Desktop/football app v3/football-app/src/reducers/teamReducer.ts",[],"/Users/jonathan/Desktop/football app v3/football-app/src/pages/Home.tsx",["129"],"import React from 'react';\nimport styled from 'styled-components';\nimport { Link } from 'react-router-dom';\nimport player from '../assets/imgs/home_page_player.png';\n\nconst HomePage = styled.div`\n  .top {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    h1 {\n      color: #212020;\n      font-weight: 500;\n      font-size: 38px;\n      letter-spacing: 2px;\n    }\n    img {\n      max-width: 60vw;\n      max-height: 40vh;\